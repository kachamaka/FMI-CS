import java.io.*;
import java.util.Random;
import java.util.Scanner;

public class vands {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		// String counter = sc.nextInt();
		int count = sc.nextInt();
		int opportunity = 0;
		for (int j = 0; j < count; j++) {
			int[] week = new int[] { 0, 0, 0, 0, 0, 0, 0 };
			for (int i = 0; i < 12; i++) {
				Random random = new Random();
				int current = 0;
				current = random.nextInt(6);
				week[current]++;
			}
			if ((week[0] != 0) && (week[1] != 0) && (week[2] != 0)
					&& (week[3] != 0) && (week[4] != 0) && (week[5] != 0)
					&& (week[6] != 0))
				opportunity++;
		}
		System.out.println("вероятността е " + opportunity + "/" + count);
	}
}
